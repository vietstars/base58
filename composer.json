{
    "name": "vietstars/base58",
    "description": "Base58 encoder and decoder for arbitrary data",
    "keywords": [
        "base58"
    ],
    "homepage": "https://github.com/vietstars/base58",
    "license": "MIT",
    "authors": [
        {
            "name": "Vietstars",
            "email": "vietstar.nt@gmail.com",
            "homepage": "https://it4today.org/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0"
    },
    "suggest": {
        "ext-gmp": "GMP extension provides the fastest encoding and decoding."
    },
    "autoload": {
        "psr-4": {
            "Vietstars\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vietstars\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    }
}
